Table Etudiants {
  immatriculation integer [primary key]
  nom varchar
  prenom varchar
  email varchar
  natel integer
}

Table Professeurs { 
  nom varchar [primary key]
  prenom varchar
  email varchar
  bureau integer
}

Table cours {
  cours_id integer [primary key]
  nom varchar
}

Table inscriptions {
  student_id integer
  cours_id integer
}

Table details_notes {
  note integer [primary key]
  nombres_evaluations integer
  date_evaluation integer
  note_recue integer
  poids_note_dans_moyenne integer
}

Ref: inscriptions.student_id > Etudiants.immatriculation
Ref: inscriptions.cours_id > cours.cours_id
Ref: cours.cours_id > Professeurs.nom
Ref: details_notes.note > inscriptions.cours_id

