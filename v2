
Table Etudiants {
  immatriculation integer [primary key]
  nom varchar
  prenom varchar
  email varchar
  natel integer
}

Table Professeurs { 
  nom varchar [primary key]
  prenom varchar
  email varchar
  bureau integer
}

Table cours {
  cours_id integer [primary key]
  nom varchar
  notes_evaluations float
}

Table inscriptions {
  student_id integer
  cours_id integer
}

Table details_notes {
  date_evaluation timestape
  note_recue float
  poids_note_dans_moyenne float
  nombre_evaluations integer
  moyenne_de_notes float
}
Table notes {
  notes_finales integer
}


Ref: inscriptions.student_id > Etudiants.immatriculation
Ref: inscriptions.cours_id > cours.cours_id
Ref: cours.cours_id > Professeurs.nom
Ref: details_notes.note_recue > cours.notes_evaluations
Ref: notes.notes_finales > details_notes.moyenne_de_notes


